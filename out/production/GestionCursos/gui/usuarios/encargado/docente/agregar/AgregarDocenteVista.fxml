<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="729.0" prefWidth="605.0" styleClass="background" stylesheets="@../../../../recursos/card/stylesheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="525.0" prefWidth="591.0" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="66.0" prefWidth="427.0">
               <children>
                  <ImageView fitHeight="52.0" fitWidth="94.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../../../../../Prototipo%20de%20diseño%20y%20construccion/PracticasProfesionales/src/org/gui/auth/resources/images/logouv.png" />
                     </image>
                  </ImageView>
               </children>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </VBox>
            <VBox prefHeight="533.0" prefWidth="533.0" spacing="10.0">
               <children>
                  <VBox prefHeight="44.0" prefWidth="619.0" spacing="10.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="37.0" prefWidth="422.0" styleClass="firstLevelLabel" text="Registrar docente" textAlignment="CENTER">
                           <font>
                              <Font name="Gill Sans" size="31.0" />
                           </font>
                        </Label>
                        <Label styleClass="firstLevelLabel" text="Información de docente">
                           <font>
                              <Font name="Open Sans Light" size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" prefHeight="57.0" spacing="5.0">
                     <children>
                        <Label text="NOMBRE" />
                        <TextField fx:id="nombreTextField" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" promptText="e.g Roberto Noble">
                           <tooltip>
                              <Tooltip id="tooltip" maxWidth="200.0" text="No debe contener carácteres extraños por ejemplo: !&quot;$%&amp;. Ni un espacio en blanco al final." textAlignment="JUSTIFY" wrapText="true" />
                           </tooltip>
                        </TextField>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" prefHeight="57.0" spacing="5.0">
                     <children>
                        <Label text="FECHA NACIMIENTO" />
