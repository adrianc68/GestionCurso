                  </VBox>
                  <VBox alignment="CENTER_LEFT" prefHeight="57.0" spacing="5.0">
                     <children>
                        <Label text="NUMERO DE PERSONAL" />
                        <TextField fx:id="numeroPersonalTextField" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" promptText="e.g A1">
